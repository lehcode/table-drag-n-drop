{
  "extends": [
    "plugin:playwright/recommended",
    "plugin:playwright/recommended",
    "plugin:@nx/react",
    "../.eslintrc.json"
  ],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "@typescript-eslint/no-explicit-any": "warn"
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["e2e/**/*.{ts,js,tsx,jsx}"],
      "rules": {}
    },
    {
      "files": ["front-e2e/**/*.{ts,js,tsx,jsx}"],
      "rules": {}
    }
  ]
}
