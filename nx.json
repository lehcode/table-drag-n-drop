{
  "targetDefaults": {
    "build": { "cache": true, "dependsOn": ["typecheck"] },
    "lint": { "cache": true },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "front:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": { "buildTarget": "front:build:development" },
        "production": { "buildTarget": "front:build:production" }
      }
    },
    "typecheck": { "cache": true },
    "@nx/js:tsc": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["default", "^default"]
    },
    "test": { "cache": true }
  },
  "defaultBase": "master",
  "installation": { "version": "19.5.6" },
  "plugins": [
    {
      "plugin": "@nx/webpack/plugin",
      "options": {
        "buildTargetName": "build",
        "serveTargetName": "serve",
        "previewTargetName": "preview"
      }
    },
    { "plugin": "@nx/eslint/plugin", "options": { "targetName": "lint" } },
    {
      "plugin": "@nx/playwright/plugin",
      "options": { "ciTargetName": "e2e-ci", "targetName": "e2e" }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": { "targetName": "test" },
      "exclude": [
        "front-e2e/**/*",
        "backend-e2e/**/*",
        "api-e2e/**/*",
        "api-e2e/**/*",
        "backend-e2e/**/*"
      ]
    }
  ],
  "generators": {
    "@nx/react": {
      "application": {
        "babel": true,
        "style": "css",
        "linter": "eslint",
        "bundler": "webpack"
      },
      "component": { "style": "css" },
      "library": { "style": "css", "linter": "eslint" }
    }
  },
  "nxCloudAccessToken": "Y2JlZDgwZDYtYmJmZS00NjU3LTk3NzUtN2Y4NDJhOTUxZjQwfHJlYWQtd3JpdGU="
}
