{
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": ["typecheck"]
    },
    "lint": {
      "cache": true
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "fetcher:build"
      },
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "buildTarget": "fetcher:build:development"
        }
      }
    },
    "typecheck": {
      "cache": true
    }
  },
  "defaultBase": "master",
  "installation": {
    "version": "19.5.4"
  },
  "plugins": [
    {
      "plugin": "@nx/webpack/plugin",
      "options": {
        "buildTargetName": "build",
        "serveTargetName": "serve",
        "previewTargetName": "preview"
      }
    },
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    },
    {
      "plugin": "@nx/playwright/plugin",
      "options": {
        "targetName": "e2e"
      }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test"
      },
      "exclude": ["backend-e2e/**/*"]
    }
  ],
  "generators": {
    "@nx/react": {
      "application": {
        "babel": true,
        "style": "css",
        "linter": "eslint",
        "bundler": "webpack"
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  }
}
